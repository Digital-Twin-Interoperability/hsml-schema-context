      {
        "@id": "http://example.com/hsml#messageCount",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Channel"
        },
        "schema:rangeIncludes": {
          "@id": "http://schema.org/Number"
