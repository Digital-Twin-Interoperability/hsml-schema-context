        "rdfs:comment": "Used to indicate smaller activities that are instances of the current activity.",
        "rdfs:label": "activityHasInstance"
      },
      {
        "@id": "http://example.com/hsml#activityIsInstanceOf",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "rdfs:comment": "Used to indicate a greater activity of which this activity is an instance of.",
        "rdfs:label": "activityIsInstanceOf"
      },
      {
        "@id": "http://example.com/hsml#activityStatus",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": {
          "@id": "http://schema.org/Text"
        },
        "rdfs:comment": "Indicates the current status or disposition of the activity. For example, planned, in progress, completed or failed.",
        "rdfs:label": "activityStatus"
      },
      {
        "@id": "http://example.com/hsml#actor",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": [
          {
            "@id": "http://example.com/hsml#Agent"
          },        
          {
            "@id": "http://example.com/hsml#Person"
          },        
          {
            "@id": "http://example.com/hsml#Organization"
          }
        ],
        "rdfs:comment": "The direct performer or driver of the action (animate or inanimate).",
        "rdfs:label": "actor"
      },      
      {
        "@id": "http://example.com/hsml#collaborator",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": {
          "@id": "http://example.com/hsml#Domain"
        },
        "rdfs:comment": "The domain (usually an Agent) with whose help this activity is performed.",
        "rdfs:label": "collaborator"
      },      
      {
        "@id": "http://example.com/hsml#condition",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": {
          "@id": "http://schema.org/Text"
        },
        "rdfs:comment": "It can be an initial condition (defined using variables), intermediate conditions or end conditions that define the activity.",
        "rdfs:label": "condition"
      },      
      {
        "@id": "http://example.com/hsml#goal",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": {
          "@id": "http://schema.org/Text"
        },
        "rdfs:comment": "The desired state or outcome of the Activity.",
        "rdfs:label": "goal"
      },      
      {
        "@id": "http://example.com/hsml#hasContract",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": {
          "@id": "http://example.com/hsml#Contract"
        },
        "rdfs:comment": "The contract under which the activity is performed. ",
        "rdfs:label": "hasContract"
      },      
      {
        "@id": "http://example.com/hsml#monitoredBy",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": [
          {
            "@id": "http://example.com/hsml#Person"
          },        
          {
            "@id": "http://example.com/hsml#Organization"
          }
        ],
        "rdfs:comment": "Organization or person in charge of monitoring and overseeing the Activity.",
        "rdfs:label": "monitoredBy"
      },      
      {
        "@id": "http://example.com/hsml#openVariables",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": {
          "@id": "http://schema.org/Text"
        },
        "rdfs:comment": "Variables that define the activity performed.",
        "rdfs:label": "openVariables"
      },      
      {
        "@id": "http://example.com/hsml#performedOn",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": [
          {
            "@id": "http://example.com/hsml#Agent"
          },        
          {
            "@id": "http://example.com/hsml#Geographic"
          },
          {
            "@id": "http://example.com/hsml#Thing"
          }
        ],
        "rdfs:comment": "The domain (whether geographic, a thing or an agent) on or in which the activity is performed.",
        "rdfs:label": "performedOn"
      },     
      {
        "@id": "http://example.com/hsml#linkedActivity",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Agent"
        },
        "schema:rangeIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "rdfs:comment": "Refers to the specific activity or task that the agent is associated with, responsible for, or engaged in.",
        "rdfs:label": "linkedActivity"
      },
      {
        "@id": "http://example.com/hsml#mainGoal",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Agent"
        },
        "schema:rangeIncludes": {
          "@id": "http://schema.org/Text"
        },
        "rdfs:comment": "Describes the primary objective, aim, or goal that the agent is intended to achieve.",
        "rdfs:label": "mainGoal"
      },
      {
        "@id": "http://example.com/hsml#access",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Channel"
        },
        "schema:rangeIncludes": {
          "@id": "http://example.com/hsml#Entity"
        },
        "rdfs:comment": "The list of entities (e.g., agents, organizations) who have access to the channel.",
        "rdfs:label": "access"
      },
      {
        "@id": "http://example.com/hsml#channelStatus",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Channel"
        },
        "schema:rangeIncludes": {
          "@id": "http://schema.org/Text"
        },
        "rdfs:comment": "The current status of the channel (e.g, active, inactive).",
        "rdfs:label": "channelStatus"
      },
      {
        "@id": "http://example.com/hsml#durationChannel",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Channel"
        },
        "schema:rangeIncludes": {
          "@id": "http://example.com/hsml#Time"
