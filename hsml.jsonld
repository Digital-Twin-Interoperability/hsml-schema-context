    "@graph": [
      {
        "@id": "http://example.com/hsml#Entity",
        "@type": "rdfs:Class",
        "rdfs:comment": "The base item in the Spatial Web Ontology.",
        "rdfs:label": "Entity"
      },
      {
        "@id": "http://example.com/hsml#Activity",
        "@type": "rdfs:Class",
        "rdfs:comment": "A single action or partially ordered set of actions performed by an Agent.",
        "rdfs:label": "Activity",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Entity"
        }
      },
      {
        "@id": "http://example.com/hsml#Channel",
        "@type": "rdfs:Class",
        "rdfs:comment": "A Channel contains HSML messages that relate to an HSML Activity.",
        "rdfs:label": "Channel",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Entity"
        }
      },
      {
        "@id": "http://example.com/hsml#Contract",
        "@type": "rdfs:Class",
        "rdfs:comment": "A binding agreement between two parties, especially enforceable by law, or a similar internal agreement wholly within an organization.",
        "rdfs:label": "Contract",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Entity"
        }
      },
      {
        "@id": "http://example.com/hsml#Credential",
        "@type": "rdfs:Class",
        "rdfs:comment": "A set of one or more claims made by a Domain. Examples of claims include identity of a Domain, membership in a Domain, or ability of a Domain to perform an Activity.",
        "rdfs:label": "Credential",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Entity"
        }
      },
      {
        "@id": "http://example.com/hsml#Domain",
        "@type": "rdfs:Class",
        "rdfs:comment": "A sphere of knowledge, influence, or Activity. A Domain retains its identity over time even though its states and relations may change. There are different Domain types, like Agent or Person.",
        "rdfs:label": "Domain",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Entity"
        }
      },
      {
        "@id": "http://example.com/hsml#Agent",
        "@type": "rdfs:Class",
        "rdfs:comment": "An individual domain with active states and agency. An Agent is an Entity that senses, responds, and maintains a model of its environment, while performing actions to achieve its goals.",
        "rdfs:label": "Agent",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Domain"
        }
      },
      {
        "@id": "http://example.com/hsml#Geographic",
        "@type": "rdfs:Class",
        "rdfs:comment": "A Domain implicitly or explicitly associated with a location.",
        "rdfs:label": "Geographic",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Domain"
        }
      },
      {
        "@id": "http://example.com/hsml#Concept",
        "@type": "rdfs:Class",
        "rdfs:comment": "A type of domain representing intangible concepts and abstract ideas shared by a community of users.",
        "rdfs:label": "Concept",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Domain"
        }
      },
      {
        "@id": "http://example.com/hsml#Organization",
        "@type": "rdfs:Class",
        "rdfs:comment": "A type of domain pertaining to membership within an entity. It can be mapped to schema:Organization.",
        "rdfs:label": "Organization",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Domain"
        },
        "owl:equivalentClass": "schema:Organization"
      },
      {
        "@id": "http://example.com/hsml#Person",
        "@type": "rdfs:Class",
        "rdfs:comment": "A special subtype of agent maintaining a self-sovereign identity. It can be mapped to schema:Person.",
        "rdfs:label": "Person",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Domain"
        },
        "owl:equivalentClass": "schema:Person"
      },
      {
        "@id": "http://example.com/hsml#Thing",
        "@type": "rdfs:Class",
        "rdfs:comment": "A type of Domain used to represent bounded items without agency. These are objects that have no goals, but that can be used or owned by other Agents to sense or perform an Activity. For example, a camera.",
        "rdfs:label": "Thing",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Domain"
        }
      },
      {
        "@id": "http://example.com/hsml#Hyperspace",
        "@type": "rdfs:Class",
        "rdfs:comment": "It represents an abstract space containing multiple domains or types of spaces. Hyperspace accounts for both physical and virtual spaces, digital twins, and Internet of Things (IoT) Entities. ",
        "rdfs:label": "Hyperspace",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Entity"
        }
      },
      {
        "@id": "http://example.com/hsml#Time",
        "@type": "rdfs:Class",
        "rdfs:comment": "It may be a dimension in spacetime, a length of a duration, a trajectory, related to a clock. When experienced by an Agent, Time has a past, present, and future. Time is related to Hyperspace or to Activities.",
        "rdfs:label": "Time",
        "rdfs:subClassOf": {
          "@id": "http://example.com/hsml#Entity"
        }
      },
      {
        "@id": "schema:creator",
        "@type": "rdf:Property",
        "schema:rangeIncludes": "http://example.com/hsml#Person"
      },
      {
        "@id": "http://example.com/hsml#linkedTo",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Entity"
        },
        "schema:rangeIncludes": {
          "@id": "http://example.com/hsml#Entity"
        },
        "rdfs:comment": "The schema of other entities to which this entity is connected.",
        "rdfs:label": "linkedTo"
      },
      {
        "@id": "http://example.com/hsml#swid",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Entity"
        },
        "schema:rangeIncludes": {
          "@id": "http://schema.org/URL"
        },
        "rdfs:comment": "A unique identifier for objects in the Spatial Web. Currently, a DID:key generated for each Entity.",
        "rdfs:label": "swid"
      },
      {
        "@id": "http://example.com/hsml#activityHasInstance",
        "@type": "rdf:Property",
        "schema:domainIncludes": {
          "@id": "http://example.com/hsml#Activity"
        },
        "schema:rangeIncludes": {
          "@id": "http://example.com/hsml#Activity"
